{
  "name": "unplugin-react-inspector",
  "version": "1.0.0",
  "description": "jump to local IDE source code while click the element of browser automatically. Supports React.",
  "author": "webfansplz",
  "license": "MIT",
  "homepage": "https://github.com/MartinBspheroid/vite-plugin-react-vue-inspector#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinBspheroid/vite-plugin-react-vue-inspector.git"
  },
  "bugs": {
    "url": "https://github.com/MartinBspheroid/vite-plugin-react-vue-inspector/issues"
  },
  "keywords": [
    "react",
    "vite",
    "vscode",
    "unplugin",
    "inspector",
    "debug"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:fix": "tsx scripts/postbuild.ts"
  },
  "dependencies": {
    "kolorist": "^1.8.0",
    "unplugin": "^1.5.0",
    "vite-plugin-react-inspector": "workspace:*"
  },
  "devDependencies": {
    "chalk": "^5.3.0",
    "fast-glob": "^3.3.1"
  }
}
