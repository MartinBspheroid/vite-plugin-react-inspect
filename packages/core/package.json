    "magic-string": "^0.30.4"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/webpack": "^5.28.5",
    "unplugin": "^1.5.0",
    "vite": "^7.0.6"
  }
